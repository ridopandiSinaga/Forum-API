{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "linebreak-style": "off",
      "no-underscore-dangle": "off",
      "class-methods-use-this": "off",
      "no-unused-vars": "off",
      "no-param-reassign": "off",
      "no-console": "off"
    }
}
